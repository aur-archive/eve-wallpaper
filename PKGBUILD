build() {
  cd "$startdir/src/"
  mkdir -p "$startdir/pkg/usr/share/wallpapers/eve-online/"
  # Installing wallpapers
  cp $startdir/src/*jpg $startdir/pkg/usr/share/wallpapers/eve-online/
  # Installing license
  install -D -m644 $startdir/src/CCP-license.txt $startdir/pkg/usr/share/licenses/$pkgname/license.txt
}

